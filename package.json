{
	"name": "middy-reroute",
	"version": "1.0.0",
	"description": "A middyjs middleware implementation of Netlify&#39;s redirects functionality.",
	"license": "MIT",
	"repository": "iDVB/middy-reroute",
	"author": {
		"name": "Dan Van Brunt",
		"email": "me@danvanbrunt.com",
		"url": "https://github.com/iDVB/middy-reroute"
	},
	"engines": {
		"node": ">=8"
	},
	"scripts": {
		"test:lint": "eslint --ignore-pattern='node_modules/' --ignore-pattern='coverage/' --ignore-pattern='docs/' .",
		"test:unit": "jest --verbose --coverage",
		"test:unit:watch": "jest --verbose --coverage --watch",
		"test": "npm run test:lint && npm run test:unit",
		"build:readme": "jsdoc2md --global-index-format grouped --template README.md.hb src/* > README.md",
		"build:docs": "jsdoc --readme README.md --package package.json --destination docs src",
		"build": "npm run build:docs && npm run build:readme",
		"release:tag": "git tag $npm_package_version && git push --tags",
		"coverage:submit": "codecov"
	},
	"main": "./index.js",
	"files": [
		"src",
		"index.js",
		"!__tests__"
	],
	"keywords": [
		""
	],
	"dependencies": {
		"axios": "^0.18.0",
		"debug": "^4.1.1",
		"deepmerge": "^3.0.0",
		"lodash.find": "^4.6.0",
		"lodash.omit": "^4.5.0",
		"lodash.reduce": "^4.6.0",
		"path-match": "^1.2.4"
	},
	"devDependencies": {
		"aws-sdk": "^2.384.0",
		"babel-eslint": "^10.0.1",
		"babel-jest": "^23.6.0",
		"babel-preset-env": "^1.7.0",
		"codecov": "^3.1.0",
		"eslint": "^5.12.0",
		"eslint-config-prettier": "^3.3.0",
		"eslint-plugin-prettier": "^3.0.1",
		"husky": "^1.3.1",
		"jest": "^23.6.0",
		"jsdoc": "^3.5.5",
		"jsdoc-to-markdown": "^4.0.1",
		"middy": "^0.20.0",
		"prettier": "^1.15.3",
		"prettier-eslint": "^8.8.2"
	},
	"jest": {
		"collectCoverageFrom": [
			"src/**/*.js"
		],
		"testEnvironment": "node"
	},
	"husky": {
		"hooks": {
			"pre-commit": "npm run build"
		}
	}
}
